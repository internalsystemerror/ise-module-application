{
    "name": "ise/ise-module-application",
    "description": "Application module for Zend Framework 2 used by Internalsystemerror Limited",
    "type": "library",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/internalsystemerror/ise-module-application",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "classmap-authoritative": false,
        "discard-changes": true,
        "optimize-autoloader": true,
        "prepend-autoloader": true,
        "process-timeout": 5000,
        "sort-packages": true
    },
    "require": {
        "php": "^7.1",
        "zendframework/zend-modulemanager": "^2.8.2",
        "zendframework/zend-mvc": "^3.1.1",
        "zendframework/zend-view": "^2.10.0"
    },
    "require-dev": {
        "doctrine/doctrine-orm-module": "^2.1.2",
        "ise/php-metapackage-development": "^2.0.2",
        "ise/ise-module-bread": "^1.0.0",
        "zendframework/zend-cache": "^2.8.2",
        "zendframework/zend-serializer": "^2.9.0"
    },
    "autoload": {
        "psr-4": {
            "Ise\\Application\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IseTest\\Application\\": "test"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-develop": "1.1.x-dev"
        }
    },
    "scripts": {
        "analyse": "phpstan analyse -l 7 config src",
        "cs-check": [
            "phpcs",
            "phpmd config,src,test html --reportfile build/logs/phpmd.html --suffixes php,phtml phpmd.xml.dist",
            "phpcpd . --exclude=assets --exclude=build --exclude=vendor "
        ],
        "cs-fix": "phpcbf",
        "test": "phpunit"
    }
}
